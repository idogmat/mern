  "bugs": {
    "url": "https://github.com/idogmat/mern/issues"
  },
  "homepage": "https://github.com/idogmat/mern#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.7",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.1",
    "react-scripts": "^5.0.1",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.19"
  }
}
